{
  "name": "arpa-js",
  "version": "1.0.0",
  "description": "Arpa 2",
  "main": "arpa.js",
  "private": true,
  "scripts": {
    "copy-files": "scripts/copy-files.sh",
    "add-ktp-koe-update": "scripts/add-ktp-koe-update.sh",
    "server:compile": "npm run copy-files && npm run add-ktp-koe-update && tsc -p server",
    "server:watch": "npm run copy-files && npm run add-ktp-koe-update && tsc -p server -w --preserveWatchOutput",
    "compile": "npm run server:compile",
    "watch": "npm run server:watch & nodemon --config nodemon.json ./bin/arpajs & wait",
    "lint": "eslint bin/arpajs . --ext .js,.ts --quiet",
    "test": "tsc --noEmit && NODE_ENV=unit-tests nyc --reporter text mocha",
    "create-migration": "create-migration",
    "db:migrate": "npm run server:compile && ./bin/run-migrations.js",
    "dev:start": "npm run server:compile && ./bin/run-migrations.js && npm run watch",
    "ci:start": "npm run server:compile && ./bin/run-migrations.js && node ./bin/arpajs",
    "start": "node ./bin/arpajs"
  },
  "author": "",
